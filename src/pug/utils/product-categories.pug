mixin categoriesList(navClass, navList = [])
    ul(class=`${navClass}-nav-list`) 
        each i in navList
          if i.url==''
            li(class=`${navClass}-nav-item list-title`) #{i.name}
          else
            li(class=`${navClass}-nav-item`)
              a(href=i.url, class=`${navClass}-nav__link`) #{i.name} (#{i.number})
