mixin navList(navClass, navList = [])
    ul(class=`${navClass}-nav-list`)
        each i in navList
          if i.url==''
            li(class=`${navClass}-nav-item list-title`) #{i.name}
          else
            li(class=`${navClass}-nav-item`)
                a(href=i.url, class=`${navClass}-nav__link`) #{i.name}

mixin workList(navList = [])
  ul.work-list
    each i in navList
      li
        a(href=`${i.url}.html`) #{i.name} 

mixin pagination(navList = [])
  ul.pagination-list
    each i in navList
      li.pagination-item
        a.pagination__link(href=`${i.url}`) #{i.name}
